package katatemplateproject;
public class KataTemplateProject {
    public static void main(String[] args) {
       System.out.println(YesOrNo.boolToWord(true));
    }
    
}
class YesOrNo
{
  public static String boolToWord(boolean b)
  {
    if(b){
        return "Yes";
    }
    else{
        return "No";
    }
  }
  
}
